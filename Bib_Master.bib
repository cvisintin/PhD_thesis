@Article{rhod14,
  Title                    = {A Few Large Roads or Many Small Ones? How to Accommodate Growth in Vehicle Numbers to Minimise Impacts on Wildlife.},
  Author                   = {Rhodes, JR and Lunney, D and Callaghan, J and McAlpine, CA},
  Journal                  = {PLoS One},
  Year                     = {2014},
  Number                   = {3},
  Pages                    = {e91093},
  Volume                   = {9},

  Abstract                 = {Roads and vehicular traffic are among the most pervasive of threats to biodiversity because they fragmenting habitat, increasing mortality and opening up new areas for the exploitation of natural resources. However, the number of vehicles on roads is increasing rapidly and this is likely to continue into the future, putting increased pressure on wildlife populations. Consequently, a major challenge is the planning of road networks to accommodate increased numbers of vehicles, while minimising impacts on wildlife. Nonetheless, we currently have few principles for guiding decisions on road network planning to reduce impacts on wildlife in real landscapes. We addressed this issue by developing an approach for quantifying the impact on wildlife mortality of two alternative mechanisms for accommodating growth in vehicle numbers: (1) increasing the number of roads, and (2) increasing traffic volumes on existing roads. We applied this approach to a koala (Phascolarctos cinereus) population in eastern Australia and quantified the relative impact of each strategy on mortality. We show that, in most cases, accommodating growth in traffic through increases in volumes on existing roads has a lower impact than building new roads. An exception is where the existing road network has very low road density, but very high traffic volumes on each road. These findings have important implications for how we design road networks to reduce their impacts on biodiversity.},
  Category                 = {Animal Collisions},
  Doi                      = {10.1371/journal.pone.0091093},
  Review                   = {Per population simulations, effect of road density is higher than road volume on traffic mortality in koalas.},
  Timestamp                = {2014.05.18}
}

