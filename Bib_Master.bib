  pages    = {1215 - 1224},
  issn     = {0022541X},
  abstract = {Road mortality of animals (roadkill) threatens public safety and wildlife populations. As mitigation tools, predictive models of roadkill are becoming more common in the published literature; however, few models generalize across multiple taxa, and thus are less useful for management scenarios that account for multiple target species. Using a dataset of 653 vertebrate roadkills collected from 2 parks in southern Ontario, we constructed generalized linear mixed models to determine the simultaneous risk factors for bird, frog, mammal, five-lined skink ( Eumeces fasciatus), snake, toad, and turtle hatchling roadkills from among a set of 8 potential predictor variables. Posted road speed limit was the dominant roadkill predictor (positive coefficient), followed by maximum daily temperature (positive), habitat diversity (positive), and distance from wetlands (negative). All else being equal, as road speed limits increase from 20 km/hr to 50 km/hr, the model predicted the season's mean road},
  category = {Animal Collisions},
  journal  = {The Journal of Wildlife Management},
  keywords = {RESEARCH, ANIMALS -- Population biology, VERTEBRATES, HABITAT (Ecology), ROADKILL, PUBLIC safety, EUMECES fasciatus, PREDICTION models, ONTARIO, Southern, amphibians, birds, generalized linear mixed models, mammals, reptiles, road mortality, roadkill, southern Ontario},
}

