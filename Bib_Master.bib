@InProceedings{ramp10,
  Title                    = {Roads as drivers of change for macropodids},
  Author                   = {Ramp, D.},
  Booktitle                = {Macropods: The Biology Of Kangaroos, Wallabies And Rat-kangaroos},
  Year                     = {2010},

  Address                  = {Collingwood, VIC, Australia},
  Editor                   = {Coulson, G and Eldridge, M},
  Pages                    = {279-291},
  Publisher                = {CSIRO Publishing},

  Abstract                 = {As an iconic and predominantly endemic Australian family of mammals, the Macropodidae are synonymous with Australia's faunal identity. Of the 53 species known prior to European settlement, six have since gone extinct and many more have suffered significant reductions in range and abundance. Although individual threats each take their toll (e. g. human modification of the landscape, hunting, the influx of invasive predators), it is the combined impact of multiple drivers of change that is of concern. This cumulative impact has the potential to expose even species typically considered common to extinction risk. Recent worldwide attention has been focused on the threat of roads to fauna, a threatening process now seen as a potential tipping-point for species struggling to survive. This chapter discusses what is known of the role that roads play in threatening species in the Macropodidae. It examines the direct and indirect effects of roads by collating published information of fatalities (road-kill), landscape modification and population persistence, contrasting impacts in urban and rural environments. It provides evidence that the impact of roads is more pervasive and potentially catastrophic for some species than previously thought, discusses attempts to mitigate the impacts of roads on fauna and explores the long-term conservation implications for the Macropodidae.},
  Affiliation              = {{Ramp, D (Reprint Author), Univ New S Wales, Sch Biol Earth \& Environm Sci, Sydney, NSW 2052, Australia. Univ New S Wales, Sch Biol Earth \& Environm Sci, Sydney, NSW 2052, Australia.}},
  Category                 = {Animal Collisions},
