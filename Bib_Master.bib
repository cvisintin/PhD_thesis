@Article{caug64,
  Title                    = {Social organization and daily activity of the red kangaroo and the grey kangaroo},
  Author                   = {Caughley, Graeme},
  Journal                  = {Journal of Mammalogy},
  Year                     = {1964},
  Pages                    = {429--436},
  Volume                   = {45},

  Abstract                 = {Although red kangaroos, Macropus rufus, and grey kangaroos, M. canguru, each range in groups, they show few of the attributes of sociality usually found in gregarious mammals. An analysis of group-size frequencies implies that the size of groups is determinedby an essentially randomprocess of animals joining and leaving, the composition of groups being in a continual state of change. This suggests that the mean group size may be a useful index to density. The red kangaroo (Macropus rufus) and the grey kangaroo (Macropus canguru) have similar mean daily schedules of activity and rest, but grey kangaroos visit water about 3 times as often as reds, the latter species being better adapted to desert and semidesert conditions.},
  Category                 = {Eastern Grey Kangaroo},
  Publisher                = {JSTOR},
  Review                   = {Grey kangaroos drink more frequently than reds.}
}

@Article{caug64a,
  Title                    = {Density and dispersion of two species of kangaroo in relation to habitat},
  Author                   = {Caughley, GJ},
  Journal                  = {Australian Journal of Zoology},
  Year                     = {1964},
  Number                   = {2},
  Pages                    = {238--249},
  Volume                   = {12},

  Abstract                 = {The dispersion of red and grey kangaroo faeces is usually random in any one plant association although the animals themselves are contagiously distributed. However, groups of kangaroos show a random dispersion in contrast to the con- tagious dispersion of groups of sheep. The density and dispersion of grey kangaroo faeces correlates with the thickness of cover, probably reflecting selection in the past by marsupial carnivores which are now extinct on the mainland. The density and dispersion of neither kangaroo species correlates with the density or dispersion of the woodland canopy. Although both species of kangaroo utilize all of the plant associations studied, there is a quantitative difference in preferred habitat in that the ratio of densities differs between associations.},
  Category                 = {Eastern Grey Kangaroo},
  Publisher                = {CSIRO},
  Review                   = {Grey kangaroos density and dispersal is related to vegetation structure and cover.}
}

@Article{caug85,
  Title                    = {The effect of drought on kangaroo populations},
  Author                   = {Caughley, Graeme and Grigg, Gordon C and Smith, Lyn},
  Journal                  = {The Journal of Wildlife Management},
  Year                     = {1985},
  Pages                    = {679--685},
  Volume                   = {49},

  Abstract                 = {Kangaroos (Macropus rufus, M. fuliginosus, and M. giganteus) declined by about 40% across more than a million square kilometersof inland eastern Australia duringa drought between April 1982 and March 1983. Most of the deaths occurred within 4 summer months over which kangaroos declined at an instantaneous rate of r = -0.135 on a monthly basis. The rate of decline showed no correlationwith the density of kangaroos. We suggest that such droughtsand the consequent mortalityand reduced fecundity are intrinsic to the ecology of kangaroos,that these occur locally almost every year and regionally about every 10 years, and that they counterbalanceto some extent the rapid increase in numbers between the droughts.},
  Category                 = {Eastern Grey Kangaroo},
  Publisher                = {JSTOR},
  Review                   = {Droughts effect kangaroos through reduced vegetation quality.}
}

@Article{caug87,
  Title                    = {Kangaroos and climate: an analysis of distribution},
  Author                   = {Caughley, Graeme and Short, Jeff and Grigg, GC and Nix, H},
  Journal                  = {The Journal of Animal Ecology},
  Year                     = {1987},
  Pages                    = {751--761},
  Volume                   = {56},

  Abstract                 = {(1) The distributionsof three partially sympatric kangaroo species, Macropus giganteus Shaw,M.fuliginosus (Desmarest) and M. rufus (Desmarest), were analysed to determine their climatic characteristics. (2) M. giganteus occupies areas only where rainfall either has little seasonal trend or where rainfall in summer exceeds winter rainfall. (3) M. fuliginosus is found in areasof uniform or winter rainfall. (4) Seasonality of rainfall has little influence on the distribution of M. rufus. Instead, its distribution reflects interaction betweenmean annual precipitation and mean annual temperature. (5) The extent of sympatry and allopatry appears to be determined by the independent reaction of each species to specific and differing climatic stimuli rather than by biological interaction between species.},
  Category                 = {Eastern Grey Kangaroo},
  Publisher                = {JSTOR},
  Review                   = {Distribution of EGK related to seasonal rainfall; where summer rainfall predominates.}
}

